<?xml version="1.0" encoding="UTF-8" ?>
<case app="GenCase4 v4.0.069 (21-05-2018)" date="11-07-2018 01:16:29">
    <casedef>
        <constantsdef>
            <gravity x="0" y="0" z="-9.81" comment="Gravitational acceleration" units_comment="m/s^2" />
            <rhop0 value="1000" comment="Reference density of the fluid" units_comment="kg/m^3" />
            <rhopgradient value="2" comment="Initial density gradient 1:Rhop0, 2:Water column, 3:Max. water height (default=2)" />
            <hswl value="0" auto="true" comment="Maximum still water level to calculate speedofsound using coefsound" units_comment="metres (m)" />
            <gamma value="7" comment="Polytropic constant for water used in the state equation" />
            <speedsystem value="0" auto="true" comment="Maximum system speed (by default the dam-break propagation is used)" />
            <coefsound value="20" comment="Coefficient to multiply speedsystem" />
            <speedsound value="0" auto="true" comment="Speed of sound to use in the simulation (by default speedofsound=coefsound*speedsystem)" />
            <coefh value="0.75" comment="Coefficient to calculate the smoothing length (h=coefh*sqrt(3*dp^2) in 3D)" />
            <cflnumber value="0.2" comment="Coefficient to multiply dt" />
        </constantsdef>
        <mkconfig boundcount="240" fluidcount="9" />
        <geometry>
            <definition dp="0.05" units_comment="metres (m)"> 
				<pointmin x="-0.05" y="-0.05" z="-0.15" />
                <pointmax x="15.05" y="1.05" z="1.55" />
            </definition>
            <commands>
                <mainlist>
                    <setshapemode>actual | dp | bound</setshapemode>
                    <setdrawmode mode="full" />
                    <setmkfluid mk="0" />
                    <drawbox>
                        <boxfill>solid</boxfill>
                        <point x="0.0" y="0.0" z="0.0" />
                        <size x="3.0" y="1.0" z="0.3" />
                    </drawbox>
                    <setmkbound mk="51" />					
					<drawfilestl file="BLIR3.stl">
						<drawscale x="0.04" y="0.04" z="0.04"/>
						<drawmove x="8.5" y="0.5" z="0.1"/>
					</drawfilestl>
					<fillbox x="8.5" y="0.51" z="0.11" mkbound="51">
                        <modefill>void</modefill>
                        <point x="8.0" y="0.0" z="-0.05" />
                        <size x="1.0" y="1.0" z="0.2" />
					</fillbox>					
					<setdrawmode mode="face" />
                    <setmkbound mk="0" />
					<setfrdrawmode auto="true" />
                    <drawfilestl file="Canal_Playa_1esa20_750cm.stl" />
					<setfrdrawmode auto="false" />
					<shapeout file="beach" />
                </mainlist>
            </commands>
        </geometry>
        <floatings>
            <floating mkbound="51-53" property="pvc" >
                <massbody value="1.06053" />  !-- better to impose mass than density (800 kg/m^3) since mass will not change with "dp"
            </floating>
        </floatings>
        <properties>
            <propertyfile file="Floating_Materials.xml" path="materials" />
            <links>
                <link mkbound="0" property="steel" comment="Property for the tank" />
            </links>
        </properties>
    </casedef>
    <execution>
        <special>
            <chrono>
                <savedata value="0.001" comment="Saves CSV with data exchange for each time interval (0=all steps)" />
                <schemescale value="1" comment="Scale used to create the initial scheme of Chrono objects (default=1)" />
				<collision activate="true">
				    <distancedp value="0.5" comment="Allowed collision overlap according Dp (default=0.5)" />
					<contactmethod value="0" comment="Contact method type. 0:NSC (Non Smooth Contacts), 1:SMC (SMooth Contacts). (default=0)" />	
				</collision>
				<bodyfloating id="BLIR" mkbound="51" modelfile="AutoActual" />
                <bodyfixed id="beach" mkbound="0" modelfile="AutoActual" modelnormal="invert" />
            </chrono>
			<gauges>
				<default>
					<Savevtkpart value="false" />
					<computedt value="0.001" unit_comment="s"/>
					<computetime start="0.0" end="10" unit_comment="s"/>
